    @model Jogador
    @{
        ViewData["Title"] = "Jogadores";
    }
    <form method="POST" action='@Url.Action("Cadastrar")'>
    <label>ID do Jogador</label>
    <input type="text" name="IdJogador" />

    <label>Nome do Jogador</label>
    <input type="text" name="Nome" />

    <label>ID da Equipe</label>
    <input type="text" name="IdEquipe" />

    <button type="submit">Cadastrar</button>
</form>

<table class="table table-striped table-responsive">
    <thead>
        <th>ID do Jogador</th>
        <th>Nome do Jogador</th>
        <th>ID da Equipe</th>
    </thead>
    <tbody>
        @foreach(Jogador j in ViewBag.Jogadores){
            <tr>
                
                <td>@j.IdJogador</td>
                <td>@j.Nome</td>
                <td>@j.IdEquipe</td>
            </tr>
        }
    </tbody>
</table>